{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000 --host localhost",
    "dev4200": "vite --port 4200 --host localhost", 
    "backend": "node backend/server.js --port 4001", 
    "backend4200": "node backend/server.js --port 4200",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "create-component": "node scripts/create-component.js"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^10.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "node scripts/check-component-imports.js"
    ]
  }
}